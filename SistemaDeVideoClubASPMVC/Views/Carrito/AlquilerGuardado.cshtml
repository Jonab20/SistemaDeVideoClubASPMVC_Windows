@{ ViewBag.Title = "AlquilerGuardado"; }


<h2 style="text-align: center">Items de Alquiler Guardados </h2>
<table style="margin: 0px auto">
    <tr>
        <td>Alquiler Nro:</td>
        <td style="font-weight: bold">: @ViewBag.AlquilerId</td>
    </tr>
</table>
@using (Html.BeginForm())
{
<div style="padding: 10px 0px; text-align: center">
    <td style="margin-left: 9px;">@Html.ActionLink("Generar Pdf", "GeneratePdf", "Carrito", new { id = @ViewBag.AlquilerId }, new { @class = "btn btn-warning", @target = "_blank" })</td>
    <td style="margin-left: 9px;">@Html.ActionLink("Volver", "Index", "Peliculas", "", new { @class = "btn btn-success" })</td>
</div>}